pipeline {
   agent any
   stages {
      stage('git checkout') {
         steps {
            git 'https://github.com/shaikshareefmastan/apipro.git'
         }
       }
      stage('Build application') {
         steps {
	            sh "docker exec -it apiserver newman run http://52.66.213.30/collection.json -r htmlextra --reporter-htmlextra-export  myreport.html"
                 }
               }
   }
}
