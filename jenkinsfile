pipeline {
   agent any
   stages {
      stage('git checkout') {
         steps {
            git 'https://github.com/shaikshareefmastan/apipro.git'
         }
       }
      stage('Build application') {
         steps {
	            sh "docker exec -it apiserver newman run http://52.66.213.30/collection.json -r htmlextra --reporter-htmlextra-export  myreport.html"
                 }
               }
         stage('postbuild'){
                 steps {
                         publishHTML (target: [
      allowMissing: false,
      alwaysLinkToLastBuild: false,
      keepAll: true,
      reportDir: '/var/lib/jenkins/workspace/apipro_pipeline',
      reportFiles: 'myreport.html'
    ])

           }

      }
   }
}
